
@{
    ViewData["Title"] = "View";
    string userId = TempData.Peek("UserId").ToString();
}

<h1 id="userName">@userId</h1>
<div id="tblNotification"></div>
<div id="tblCertificate"></div>
<div>
   
    <div>
        <div class="row">
            <div class="col-md-8">
                <div class="x_panel">
                    <div class="x_title">
                        <h4>Requests</h4>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <table  id="tblRequest">
                            <thead>
                                <tr>
                                    <th>id</th>
                                    <th>userId</th>
                                    <th>statusId</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/js/microsoft/signalr/dist/browser/signalr.js"></script>
    <script src="~/js/signalr.js"></script>
}